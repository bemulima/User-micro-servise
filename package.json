{
  "name": "user-service",
  "version": "1.0.0",
  "type": "module",
  "main": "dist/main.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/main.ts",
    "build": "tsc -p tsconfig.build.json",
    "start": "node dist/main.js",
    "lint": "eslint .",
    "test": "jest --passWithNoTests",
    "migrate:run": "ts-node ./node_modules/typeorm/cli.js migration:run -d src/infrastructure/orm/typeorm.config.ts",
    "migrate:revert": "ts-node ./node_modules/typeorm/cli.js migration:revert -d src/infrastructure/orm/typeorm.config.ts"
  },
  "dependencies": {
    "@fastify/cors": "^10.0.1",
    "@fastify/helmet": "^12.0.0",
    "@fastify/jwt": "^9.0.1",
    "@fastify/oauth2": "^7.8.1",
    "@fastify/rate-limit": "^10.1.0",
    "@fastify/sensible": "^6.0.1",
    "@fastify/swagger": "^9.3.1",
    "@fastify/swagger-ui": "^3.0.0",
    "argon2": "^0.40.1",
    "amqplib": "^0.10.4",
    "dotenv": "^16.4.5",
    "fastify": "^4.28.1",
    "fastify-type-provider-zod": "^1.2.0",
    "prom-client": "^15.1.2",
    "pino": "^9.3.1",
    "reflect-metadata": "^0.2.1",
    "tsyringe": "^4.8.0",
    "typeorm": "^0.3.20",
    "pg": "^8.13.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^22.7.5",
    "@typescript-eslint/eslint-plugin": "^8.7.0",
    "@typescript-eslint/parser": "^8.7.0",
    "eslint": "^9.11.1",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.6.3"
  }
}
